---
import Config from "../sarissa.config.cjs";

const { pageTitle } = Astro.props;

const title = pageTitle + " - " + Config.siteTitle;
---

<head>
  <meta charset="utf-8" />
 <meta name="viewport" content="width=device-width,initial-scale=1" />
 <meta name="description" content={Config.description}>
  <title>{title}</title>
<link rel="canonical" href="https://codealagreek.netlify.app/" />
  <link rel="icon" href={Config.favicon}>
  <script async src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
</head>
